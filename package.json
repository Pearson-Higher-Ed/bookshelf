    "lodash": "^4.17.2",
    "material-ui": "^0.16.7",
    "mocha": "~2.4.5",
    "node-sass": "^4.5.3",
    "pearson-elements": "^0.10.0",
    "prop-types": "^15.5.10",
    "pseudoloc": "^1.1.0",
    "react": "^15.4.0",
    "react-addons-test-utils": "^15.4.0",
    "react-dimensions": "^1.3.0",
    "react-dom": "^15.4.0",
